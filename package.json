{
  "name": "logd_app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "aws-amplify": "^2.2.1",
    "react": "^16.10.2",
    "react-bootstrap": "^1.0.0-beta.16",
    "react-dom": "^16.10.2",
    "react-quill": "^1.3.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.3.0"
  },
  "scripts": {
    "dev": "react-scripts start",
    "dev:test": "REACT_APP_STAGE=TEST yarn dev",
    "build": "react-scripts build",
    "build:dev": "REACT_APP_STAGE=DEV yarn build",
    "build:prod": "REACT_APP_STAGE=PROD yarn build",
    "serve:test": "REACT_APP_STAGE=TEST yarn serve -s build",
    "eject": "react-scripts eject",
    "atf": "atf -d 'src' -e 'ts' 'tsx'",
    "test": "react-scripts test --env=jsdom",
    "test:e2e:dev": "CYPRESS_BASE_URL=http://localhost:3000/ cypress open",
    "test:e2e:ci": "CYPRESS_BASE_URL=http://localhost:5000/ cypress run --record"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.0.0",
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^8.0.3",
    "@types/jest": "^24.9.0",
    "@types/node": "^12.7.12",
    "@types/react": "^16.9.5",
    "@types/react-dom": "^16.0.7",
    "@types/react-router-dom": "^5.1.3",
    "auto-test-file": "^1.2.2",
    "cypress": "^4.0.1",
    "serve": "^11.3.0",
    "ts-jest": "^24.3.0",
    "typescript": "^3.6.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
